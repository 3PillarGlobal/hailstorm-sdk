<%= form_for([@project, @cluster], html: {class: 'form-horizontal'}) do |f| %>

    <%= form_for_cluster(object: f) %>

    <br/>
    <div class="form-group">
      <div class="col-sm-3 col-sm-offset-3"><%= f.submit 'Save', class: 'btn btn-primary' %></div>
      <div class="col-sm-6"><%= link_to 'Cancel', project_clusters_path, class: 'btn btn-default pull-right' %></div>
    </div>
<% end %>

<%= content_for :project_header do %>
    <% unless @project.nil? %>
        <%= link_to @project.title, project_path(@project) %>
    <% end %>
<% end %>
<h2 class="page-header">Add Target Hosts</h2>
<br/>
<div class="col-md-6 col-md-offset-3">
  <% if @target_host_data.blank? %>
      <%= render 'addform' %>
  <% else %>
      <%= render 'updateform' %>
  <% end %>
</div>
